  }
}
/*  footer
===================================================================*/
.pagetop a {
  position: fixed;
  right: 20px;
  bottom: 20px;
  display: block;
  width: 60px;
  height: 60px;
  background: #e50038;
  z-index: 2;
  text-indent: -9999px;
  transition: all .3s;
  opacity: 0.7;
  -moz-opacity: 0.7;
  -webkit-opacity: 0.7;
}
.pagetop a:after {
  display: block;
  position: absolute;
  top: 50%;
  right: 50%;
  width: 18px;
  height: 18px;
  margin-top: -6px;
  margin-right: -9px;
  border-top: solid 2px #fff;
  border-right: solid 2px #fff;
  -webkit-transform: rotate(-45deg);
  transform: rotate(-45deg);
  content: "";
}
.pagetop a:hover {
  opacity: 1;
  -moz-opacity: 1;
  -webkit-opacity: 1;
}

@media screen and (max-width: 767px) {
  .pagetop a {
    right: 10px;
    bottom: 10px;
    width: 40px;
    height: 40px;
  }
  .pagetop a:after {
    width: 14px;
    height: 14px;
    margin-right: -7px;
  }
}
footer {
  padding: 40px 0 40px;
  background: #ff9000;
  text-align: center;
  color: #fff;
}
footer a {
  transition: all .3s;
  color: #fff;
}
footer a:hover {
  opacity: 0.8;
  -moz-opacity: 0.8;
  -webkit-opacity: 0.8;
}
footer dt {
  padding-bottom: 40px;
  font-family: 'Cabin', sans-serif;
  font-weight: 600;
  font-size: 40px;
  line-height: 1.3;
}
footer dt span {
  border-bottom: #fff 7px solid;
}
footer dd {
  margin: 0 24px;
  display: inline-block;
}
footer dd a {
  display: block;
}
footer .bnr {
  margin: 30px 0 0;
}
footer .bnr li {
  display: inline-block;
}
footer .copy {
  padding-top: 50px;
}

@media screen and (max-width: 767px) {
  footer {
    padding: 20px 0 15px;
  }
  footer dt {
    padding-bottom: 20px;
    font-size: 20px;
  }
  footer dt span {
    border-bottom: #fff 3px solid;
  }
  footer dd {
    margin: 0 14px;
  }
  footer dd img {
    width: 22px;
  }
  footer .bnr {
    margin: 20px 0 0;
  }
  footer .bnr img {
    width: 180px;
  }
  footer .copy {
    padding-top: 30px;
    font-size: 10px;
  }
}
