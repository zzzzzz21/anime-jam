/* ■ contents共通
===================================================================*/
#contents {
  background: url(../images/bg_contents.jpg) 50% 0 repeat-x;
  background-attachment: fixed;
  color: #525252;
}
#contents img {
  display: block;
  width: 100%;
  height: auto;
}
#contents .btnCenter a {
  display: block;
  width: 480px;
  height: 80px;
  margin: 0 auto;
  text-indent: -9999px;
  transition: all .3s;
}
#contents .btnCenter a:hover {
  opacity: 0.65;
  -moz-opacity: 0.65;
  -webkit-opacity: 0.65;
}
#contents .title {
  height: 120px;
  text-align: center;
  background: url(../images/bg_block.png) 0 11px repeat;
}
#contents .title h2 {
  padding-bottom: 7px;
  color: #fff;
  font-family: 'Cabin', sans-serif;
  font-weight: 600;
  font-size: 40px;
  line-height: 1.3;
  position: relative;
  top: 50%;
  -webkit-transform: translateY(-50%);
  -ms-transform: translateY(-50%);
  transform: translateY(-50%);
}
#contents .title h2 span {
  display: inline-block;
  border-bottom: #fff 7px solid;
}
#contents #contentsBody {
  padding: 100px 40px;
  width: 1200px;
  margin: 0 auto;
  box-sizing: border-box;
}
#contents #contentsBody .post {
  max-width: 1000px;
  margin: 0 auto;
  box-sizing: border-box;
}
#contents #contentsBody .bgWhite {
  padding: 60px 50px;
  background: rgba(255, 255, 255, 0.6);
}
#contents #contentsBody .blc_3 .post {
  float: left;
  width: 30%;
  background: #fff;
  margin: 0 1% 2.2%;
}
#contents #contentsBody .blc_3 .post:nth-child(3n+1) {
  margin-left: 3%;
}
#contents #contentsBody .pager {
  margin: 115px 0 100px;
  text-align: center;
}
#contents #contentsBody .pager a, #contents #contentsBody .pager span {
  display: inline-block;
  padding: 0 5px;
  margin: 0 8px;
  font-family: 'Cabin', sans-serif;
  font-weight: 700;
  font-size: 33px;
  line-height: 1;
  vertical-align: top;
}
#contents #contentsBody .pager a.prev,
#contents #contentsBody .pager a.next {
  font-family: "Hiragino Kaku Gothic Pro", "ヒラギノ角ゴ Pro W3", "メイリオ", Meiryo, verdana,"ＭＳ Ｐゴシック", sans-serif;
  font-weight: 600;
  font-size: 30px;
}
#contents #contentsBody .pager .current, #contents #contentsBody .pager a:hover {
  color: #ff9000;
}

@media screen and (max-width: 767px) {
  #contents {
    background: url(../images/bg_contents-sp.png) 50% 0 repeat-x;
    background-size: 767px auto;
    -moz-background-size: 767px auto;
    -webkit-background-size: 767px auto;
  }
  #contents #contentsBody {
    padding: 25px 10px 20px;
    width: 100%;
    max-width: 757px;
    margin: 0 auto;
