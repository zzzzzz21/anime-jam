@charset "UTF-8";
/* reset
==========================================================================================*/
html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, img, ins, kbd, q, s, samp,
small, strike, sub, sup, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, embed,
figure, figcaption, footer, header, hgroup,
menu, nav, output, ruby, section, summary,
time, mark, audio, video {
  margin: 0;
  padding: 0;
  border: 0;
  font-style: normal;
  font-size: 100%;
  vertical-align: baseline;
}

/* HTML5 display-role reset for older browsers */
article, aside, details, figcaption, figure,
footer, header, hgroup, menu, nav, section {
  display: block;
}

html {
  overflow-y: scroll;
}

body {
  font-family: "Hiragino Kaku Gothic Pro", "ヒラギノ角ゴ Pro W3", "メイリオ", Meiryo, verdana,"ＭＳ Ｐゴシック", sans-serif;
}

blockquote, q {
  quotes: none;
}

blockquote:before, blockquote:after,
q:before, q:after {
  content: '';
  content: none;
}

input, textarea {
  margin: 0;
  padding: 0;
}

ol, ul, li {
  list-style: none;
}

table {
  border-collapse: collapse;
  border-spacing: 0;
}

caption, th {
  text-align: left;
}

a:focus {
  outline: none;
}

* html .clearfix {
  height: 1px;
  /*＼*/
     /*/
height: auto;
overflow: hidden;
/**/
}

.both {
  clear: both;
}

/*==========================================================================================

	// 基本

==========================================================================================*/
body {
  font-size: 16px;
  line-height: 1.5;
  color: #000;
  min-width: 1200px;
  background: #fff;
  word-wrap: break-word;
  -webkit-text-size-adjust: none;
}

.fontS {
  font-size: 90%;
}

.sp_con {
  display: none;
}

span.pc_txt {
  display: block;
}

@media screen and (max-width: 767px) {
  body {
    min-width: 100%;
    font-size: 14px;
    line-height: 1.5;
  }
  body .pc_con {
    display: none;
  }
  body .sp_con {
    display: block;
  }
  body span.sp_txt {
    display: block;
  }
  body span.pc_txt {
    display: inline;
  }
}
a {
  color: #555;
  text-decoration: none;
}

a:hover {
  color: #666;
}

/* clearfix */
.clearfix {
  zoom: 1;
}
.clearfix:before, .clearfix:after {
  content: "";
  display: table;
}
.clearfix:after {
  clear: both;
}

header,
footer .inner,
#contents {
  min-width: 1200px;
  margin: 0 auto;
}

@media screen and (max-width: 767px) {
  header,
  footer .inner,
  #contents {
    min-width: 100%;
    margin: 0 auto;
  }
}
/*==========================================================================================

	■ header / footer

==========================================================================================*/
/*  header 
===================================================================*/
header {
  background: #fff;
  height: 80px;
}
header nav {
  position: absolute;
  width: 100%;
  height: 80px;
  top: 0;
  z-index: 15;
  background: #fff;
}
header .logo {
  float: left;
  width: 145px;
  padding: 0 13px;
}
header .logo a {
  display: block;
  padding: 19px 0;
}
header .logo img {
  display: block;
  width: 100%;
  height: auto;
}
header .navIco {
  display: none;
}
header .gNav {
  height: 80px;
  float: right;
  width: calc(100% - 178px);
}
header .gNav li {
  float: left;
  height: 80px;
  width: calc(100% / 7);
  box-sizing: border-box;
  border-bottom: #fff 5px solid;
}
header .gNav li:hover {
  background: #fbebd9;
  border-bottom: #ff9000 5px solid;
}
header .gNav li a, header .gNav li.soon p {
  display: block;
  height: 100%;
  text-align: center;
  color: #ff9000;
  font-family: 'Cabin', sans-serif;
  font-weight: 600;
  font-size: 18px;
}
header .gNav li a span, header .gNav li.soon p span {
  display: block;
  position: relative;
  top: 50%;
  -webkit-transform: translateY(-50%);
  -ms-transform: translateY(-50%);
  transform: translateY(-50%);
}
header .gNav li a span.fontS, header .gNav li.soon p span.fontS {
  display: inline;
  position: static;
  font-size: 80%;
}
header .gNav li.soon p {
  color: silver;
}
header .gNav li.soon:hover {
  background: none;
  border-bottom: #fff 5px solid;
  border-left: none !important;
  border-right: none !important;
}

.second header nav {
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 50;
}

.navBg-sp {
  display: none;
}

@media screen and (max-width: 767px) {
  .navBg-sp {
    position: fixed;
    background: white;
    top: 0;
    left: 0;
    height: 100%;
    width: 100%;
    z-index: 10;
  }

  header {
    position: relative;
    height: 50px;
    background: #fff;
  }
  header .logo {
    z-index: 20;
    float: none;
    width: 133px;
    padding: 0 8px;
  }
  header .logo a {
    padding: 8px 0 11px;
  }
  header .navIco {
    display: block;
  }
  header .navIco .op {
    position: absolute;
    top: 0;
    right: 0;
    display: block;
    height: 50px;
    width: 50px;
    text-indent: -9999px;
    background: url(../images/nav_ico.png) 0 0 no-repeat;
    background-size: 100% auto;
    -moz-background-size: 100% auto;
    -webkit-background-size: 100% auto;
  }
  header .navIco .closeIco {
    display: none;
    position: absolute;
    top: 0;
    right: 0;
    z-index: 20;
    width: 50px;
    height: 50px;
    text-indent: -9999px;
    background: url(../images/nav_ico_close.png) 0 0 no-repeat;
    background-size: 100% auto;
    -moz-background-size: 100% auto;
    -webkit-background-size: 100% auto;
  }
  header .navIco.active {
    background: url(../images/nav_ico_close.png) 0 0 no-repeat;
    background-size: 100% auto;
    -moz-background-size: 100% auto;
    -webkit-background-size: 100% auto;
  }
  header nav {
    position: absolute;
    height: 50px;
  }
  header .gNav {
    display: none;
    height: auto;
    float: none;
    width: 100%;
  }
  header .gNav li {
    float: none;
    height: auto;
    width: 100%;
    border-bottom: #f6d3a9 2px solid;
    background: #fef8f2;
  }
  header .gNav li:first-child {
    border-top: #f6d3a9 2px solid;
  }
  header .gNav li:hover, header .gNav li.soon:hover {
    background: #fef8f2;
    border-bottom: #f6d3a9 2px solid;
  }
  header .gNav li a, header .gNav li.soon p {
    position: relative;
    display: block;
    height: auto;
    padding: 20px 10px;
    text-align: left;
    font-size: 16px;
  }
  header .gNav li a:after, header .gNav li.soon p:after {
    display: block;
    position: absolute;
    top: 50%;
    width: 11px;
    height: 13px;
    right: 10px;
    margin-top: -10px;
    content: "";
    background: url(../images/ico_nav_arrow.png) 0 0 no-repeat;
    background-size: 100% auto;
    -moz-background-size: 100% auto;
    -webkit-background-size: 100% auto;
  }
  header .gNav li a span, header .gNav li.soon p span {
    position: static;
    top: auto;
    -webkit-transform: none;
    -ms-transform: none;
    transform: none;
  }
  header .gNav li.soon p:after {
    background: url(../images/ico_nav_arrow-soon.png) 0 0 no-repeat;
    background-size: 100% auto;
    -moz-background-size: 100% auto;
    -webkit-background-size: 100% auto;
  }
}
#info .gNav li:nth-child(1) a,
#cast .gNav li:nth-child(2) a,
#news .gNav li:nth-child(3) a,
#news-detail .gNav li:nth-child(3) a,
#lineup .gNav li:nth-child(4) a,
#ticket .gNav li:nth-child(5) a,
#movie .gNav li:nth-child(6) a,
#goods .gNav li:nth-child(7) a {
  background: #fbebd9;
  border-bottom: #ff9000 5px solid;
}

@media screen and (max-width: 767px) {
  #info .gNav li:nth-child(1) a,
  #cast .gNav li:nth-child(2) a,
  #news .gNav li:nth-child(3) a,
  #news-detail .gNav li:nth-child(3) a,
  #lineup .gNav li:nth-child(4) a,
  #ticket .gNav li:nth-child(5) a,
  #movie .gNav li:nth-child(6) a,
  #goods .gNav li:nth-child(7) a {
    background: #fef8f2;
    border-bottom: none;
  }
}
/*---------- top ----------*/
#top header {
  height: 765px;
  background: #ff9000;
}
#top header nav {
